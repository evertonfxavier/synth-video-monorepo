    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@synth-video/core": "workspace:*"
  },
  "devDependencies": {
    "@synth-video/eslint-config": "workspace:*",
